{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "deploy": "cloudbase framework deploy",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.32.10",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@cloudbase/js-sdk": "^1.7.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "compression-webpack-plugin": "^9.2.0",
    "cross-env": "^7.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
